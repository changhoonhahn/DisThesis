	Author = {{Klypin}, A. and {Yepes}, G. and {Gottlober}, S. and {Prada}, F. and {Hess}, S.},
	Date-Added = {2016-02-23 19:44:23 +0000},
	Date-Modified = {2016-02-23 19:44:23 +0000},
	Eprint = {1411.4001},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics},
	Month = nov,
	Title = {{MultiDark simulations: the story of dark matter halo concentrations and density profiles}},
	Year = 2014,
	Bdsk-File-1 = {},
