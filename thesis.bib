	Eprint = {astro-ph/0106120},
	Journal = {\mnras},
	Keywords = {METHODS: NUMERICAL, LARGE-SCALE STRUCTURE OF UNIVERSE},
	Month = jan,
	Pages = {629-640},
	Title = {{PTHALOS: a fast method for generating mock galaxy distributions}},
