	Date-Added = {2016-08-30 13:50:20 +0000},
	Date-Modified = {2016-08-30 13:50:20 +0000},
	Eprint = {1605.04788},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics},
	Month = may,
	Title = {{Improving fast generation of halo catalogs with higher-order Lagrangian perturbation theory}},
