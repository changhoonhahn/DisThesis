	Doi = {10.1016/S0370-1573(02)00135-7},
	Eprint = {astro-ph/0112551},
	Journal = {\physrep},
	Month = sep,
	Pages = {1-248},
	Title = {{Large-scale structure of the Universe and cosmological perturbation theory}},
	Volume = 367,
