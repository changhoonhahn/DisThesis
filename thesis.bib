	Author = {{Tinker}, J. and {Wetzel}, A. and {Conroy}, C.},
	Date-Added = {2016-03-14 19:47:13 +0000},
	Date-Modified = {2016-03-14 19:47:13 +0000},
	Eprint = {1107.5046},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Cosmology and Extragalactic Astrophysics},
	Month = jul,
	Primaryclass = {astro-ph.CO},
